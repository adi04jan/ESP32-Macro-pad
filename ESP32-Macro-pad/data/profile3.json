{
    "profile_name": "Log Parsing & Diagnostics",
    "idle_animation": "none",
    "default_delay": 20,
    "keys": [
        {
            "id": 1,
            "name": "grep keyword",
            "led_color": [
                255,
                200,
                0
            ],
            "actions": [
                {
                    "type": "text",
                    "value": "grep -Rin "
                }
            ]
        },
        {
            "id": 2,
            "name": "grep 10 lines above",
            "led_color": [
                255,
                160,
                0
            ],
            "actions": [
                {
                    "type": "text",
                    "value": "egrep -B10 "
                }
            ]
        },
        {
            "id": 3,
            "name": "grep JSON keys",
            "led_color": [
                255,
                180,
                0
            ],
            "actions": [
                {
                    "type": "text",
                    "value": "grep -R \"\\\"key\\\":\""
                }
            ]
        },
        {
            "id": 4,
            "name": "compare two logs",
            "led_color": [
                255,
                150,
                0
            ],
            "actions": [
                {
                    "type": "text",
                    "value": "diff -u file1.log file2.log"
                },
                {
                    "type": "key",
                    "value": "ENTER"
                }
            ]
        },
        {
            "id": 5,
            "name": "jq validate",
            "led_color": [
                255,
                120,
                0
            ],
            "actions": [
                {
                    "type": "text",
                    "value": "jq '.'"
                }
            ]
        },
        {
            "id": 6,
            "name": "JSON diff 5 keys",
            "led_color": [
                255,
                200,
                20
            ],
            "actions": [
                {
                    "type": "multiline",
                    "value": "jq -r 'paths | select(length == 2)'"
                }
            ]
        },
        {
            "id": 7,
            "name": "cut columns",
            "led_color": [
                255,
                180,
                50
            ],
            "actions": [
                {
                    "type": "text",
                    "value": "cut -d',' -f1,2,3 "
                }
            ]
        },
        {
            "id": 8,
            "name": "awk filter",
            "led_color": [
                255,
                120,
                30
            ],
            "actions": [
                {
                    "type": "text",
                    "value": "awk '{print $1, $2, $NF}' "
                }
            ]
        },
        {
            "id": 9,
            "name": "sort unique",
            "led_color": [
                255,
                100,
                0
            ],
            "actions": [
                {
                    "type": "text",
                    "value": "sort | uniq -c"
                }
            ]
        },
        {
            "id": 10,
            "name": "filter warnings",
            "led_color": [
                255,
                140,
                40
            ],
            "actions": [
                {
                    "type": "text",
                    "value": "egrep -i 'warn|timeout|delay'"
                }
            ]
        },
        {
            "id": 11,
            "name": "extract timestamp",
            "led_color": [
                255,
                160,
                60
            ],
            "actions": [
                {
                    "type": "text",
                    "value": "grep -oE '[0-9]{4}-[0-9]{2}-[0-9]{2} [0-9:]{8}' "
                }
            ]
        },
        {
            "id": 12,
            "name": "log summary",
            "led_color": [
                255,
                200,
                100
            ],
            "actions": [
                {
                    "type": "text",
                    "value": "grep -R \"ERROR\" | wc -l"
                },
                {
                    "type": "key",
                    "value": "ENTER"
                }
            ]
        }
    ]
}